@model IEnumerable<ResidualCenter.Models.ServiceType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CreateServiceType")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditServiceType", new { id = item.ID }) |
                @Html.ActionLink("Delete", "DeleteServiceType", new { id = item.ID })
            </td>
        </tr>
    }

</table>
